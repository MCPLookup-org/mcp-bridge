# MCPLookup Bridge Environment Configuration
# Copy this file to .env and fill in your values

# MCPLookup API Configuration
MCPLOOKUP_API_KEY=mcp_your_api_key_here
MCPLOOKUP_BASE_URL=https://mcplookup.org/api/v1

# Node.js Environment
NODE_ENV=production

# Optional: Custom port for HTTP mode
PORT=3000

# Optional: Redis configuration (if using caching)
REDIS_URL=redis://redis:6379

# Optional: Logging level
LOG_LEVEL=info

# Optional: Custom user agent
USER_AGENT=MCPLookup-Bridge/1.0.0

# Optional: Request timeout (milliseconds)
REQUEST_TIMEOUT=30000

# Optional: Rate limiting
RATE_LIMIT_REQUESTS=1000
RATE_LIMIT_WINDOW=3600000

# Optional: Health check endpoint
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PATH=/health
